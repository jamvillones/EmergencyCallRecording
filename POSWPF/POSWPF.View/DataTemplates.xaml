<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ECR.WPF.ViewModels"
                    xmlns:uc="clr-namespace:ECR.WPF.Views.UserControls">
    <DataTemplate DataType="{x:Type vm:Form_Add_Record_ViewModel }">
        <uc:RecordsForm />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:Form_Edit_Record_ViewModel}">
        <uc:RecordsForm />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:Form_Add_Agency_ViewModel}">
        <uc:AgencyForm />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:Form_Edit_Agency_ViewModel}">
        <uc:AgencyForm />
    </DataTemplate>

</ResourceDictionary>